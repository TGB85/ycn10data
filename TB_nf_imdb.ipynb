    "def unique_genres(column):\n",
    "    genres_set = set()\n",
    "    for entry in column:\n",
    "        list = entry.split(',')\n",
    "        for item in list:\n",
    "            genres_set.add(item.strip())\n",
    "    return genres_set"
   ]
